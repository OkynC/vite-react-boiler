{
  "$schema": "http://json.schemastore.org/prettierrc",
  "importOrder": [
    "^@nestjs/(.*)$",
    "^react",
    "^@mui/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^\\.(.*)"
  ],
  "importOrderSeparation": true,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
